    "fix:style": "run-s 'lint:style --fix'",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.6.1",
    "@storybook/addon-interactions": "7.6.1",
    "@storybook/addon-links": "7.6.1",
    "@storybook/blocks": "7.6.1",
    "@storybook/react": "7.6.1",
    "@storybook/react-vite": "7.6.1",
    "@storybook/test": "7.6.1",
    "@types/lodash": "^4.14.202",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "@vitejs/plugin-react": "4.2.0",
    "alias-hq": "6.2.3",
    "eslint": "8.53.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.4",
    "eslint-plugin-ssr-friendly": "1.3.0",
    "eslint-plugin-storybook": "0.6.15",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.0",
    "storybook": "7.6.1",
    "stylelint": "15.11.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.2.2",
    "vite": "5.0.4"
  }
