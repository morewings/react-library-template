{
  "name": "react-library-template",
  "homepage": "https://morewings.github.io/react-library-template/",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "keywords": [
    "react",
    "template",
    "react-testing-library",
    "testing-library",
    "eslint",
    "npm",
    "rollup",
    "stylelint",
    "library",
    "package",
    "vite",
    "css-modules",
    "typescript"
  ],
  "description": "Missing React, typescript NPM library creation template",
  "scripts": {
    "dev": "vite",
    "clean:lib": "rm -rf dist",
    "build:lib": "vite build",
    "build": "run-s clean:lib build:lib",
    "lint:code": "eslint src/** --report-unused-disable-directives",
    "fix:code": "run-s 'lint:code --fix'",
    "lint:types": "tsc --pretty --noEmit",
    "lint:style": "stylelint ./src/**/*.css",
    "fix:style": "run-s 'lint:style --fix'",
    "test": "jest",
    "preview": "vite preview",
    "start:docs": "storybook dev -p 6006",
    "build:docs": "storybook build",
    "generate:component": "npx generate-react-cli component",
    "prepare": "is-ci || husky",
    "lint-staged": "lint-staged"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@eslint/compat": "1.3.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.31.0",
    "@storybook/addon-docs": "9.0.18",
    "@storybook/addon-links": "9.0.18",
    "@storybook/builder-vite": "9.0.18",
    "@storybook/react-vite": "9.0.18",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "@vitejs/plugin-react": "4.7.0",
    "@yelo/rollup-node-external": "1.0.1",
    "alias-hq": "6.2.4",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-ssr-friendly": "1.3.0",
    "eslint-plugin-storybook": "9.0.18",
    "generate-react-cli": "8.4.9",
    "husky": "9.1.7",
    "identity-obj-proxy": "3.0.0",
    "is-ci": "4.1.0",
    "jest": "30.0.5",
    "jest-environment-jsdom": "30.0.5",
    "lint-staged": "16.1.2",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "postcss-preset-env": "10.2.4",
    "prettier": "3.6.2",
    "react": "19.1.0",
    "react-docgen-typescript": "2.4.0",
    "react-dom": "19.1.0",
    "storybook": "9.0.18",
    "stylelint": "16.22.0",
    "stylelint-config-standard": "38.0.0",
    "stylelint-order": "7.0.0",
    "stylelint-prettier": "5.0.3",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.38.0",
    "vite": "7.0.5",
    "vite-plugin-dts": "4.5.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}
