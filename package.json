{
  "name": "react-library-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint:code": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "fix:code": "run-s 'lint:code --fix'",
